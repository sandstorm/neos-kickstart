prototype(MyVendor.AwesomeNeosProject:Component.Headline) < prototype(Neos.Fusion:Component) {
    title = ''

    @propTypes {
        @strict = true
        title = ${PropTypes.string}
    }

    renderer = Neos.Fusion:Component {
        @apply.props = ${props}
        renderer = afx`
            <div class="headline">{props.title}</div>
        `
    }
}
