prototype(MyVendor.AwesomeNeosProject:Component.Columns) < prototype(Neos.Fusion:Component) {
    content = ''

    @propTypes {
        @strict = true
        content = ${PropTypes.string}
    }

    renderer = Neos.Fusion:Component {
        @apply.props = ${props}
        renderer = afx`
            <div class="columns">
                {props.content}
            </div>
        `
    }
}
