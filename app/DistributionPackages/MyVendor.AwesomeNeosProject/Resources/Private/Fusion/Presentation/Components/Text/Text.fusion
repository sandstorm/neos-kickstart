prototype(MyVendor.AwesomeNeosProject:Component.Text) < prototype(Neos.Fusion:Component) {
    text = ''

    @propTypes {
        @strict = true
        text = ${PropTypes.string}
    }

    renderer = Neos.Fusion:Component {
        @apply.props = ${props}
        renderer = afx`
            <div>{props.text}</div>
        `
    }
}
